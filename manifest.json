{
    "manifest_version": 3,
    "name": "Clean Eye",
    "version": "1.0",
    "type": "module",
    "description": "현재 페이지의 HTML을 가져오는 확장 프로그램",
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}